

function info() {
    echo "Docker image -> angular"
    echo "angular -> `ng version`"
    echo "nvm -> `nvm --version`"
    echo "nvm which current -> `nvm which current`"
    echo "node -> `node --version`"
    echo "npm -> `npm --version`"
    echo "corepack -> `corepack --version`"
    echo "pnpm -> `pnpm --version`"
    echo "vite -> `vite --version`"
}
